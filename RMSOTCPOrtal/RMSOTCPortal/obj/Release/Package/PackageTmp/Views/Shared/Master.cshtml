<!DOCTYPE HTML>
<html>
<head>
    <link rel="icon" href="~/Image/download.jfif" type="image/gif" sizes="50x50">
    <title>RMS OTC Portal</title>
</head>

<body>

    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#" style="color: #fff; font-family: Arial Rounded MT; font-size: 20px">RMS OTC Portal</a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                     @*<li><a>
                        <div class="bell">
                            <div class="questionMarkFLM" data-toggle="modal" data-target=".ForFLMQuestion" id="FLMQuestion" title="FLM Details"><i class="fa fa-question-circle" style="color:white"></i></div>
                           
                        </div>
                    </a>
                   &nbsp &nbsp &nbsp &nbsp </li>*@
                     @*<li><a>
                        <div class="bell">
                            <div class="questionMark" data-toggle="modal" data-target=".ForQuestion" id="Question"><i class="fa fa-question" style="color:white"></i></div>
                        </div>
                    </a>
                   &nbsp &nbsp &nbsp &nbsp </li>*@
                     <li><a>
                        <div class="bell">
                            <div class="notification"></div>
                            @*<button >Increment Notifications</button>*@
                        </div>
                    </a>
                    </li>
                    <li>
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" style="color: #fff;">@Session["UserName"]<b class="caret"></b></a>
                        <ul class="dropdown-menu multi-level">
                            <li><a href="~/ChangePassword">Change Password</a></li>
                            <li><a href="~/Logout">Logout</a></li>
                        </ul>
                    </li>
                    @* <li><a href="#" target="_blank" style="color:#fff;">Shiva Brata Giri</a></li>*@
                </ul>
                <ul class="nav navbar-nav">
                    @if (Session["UserType"].ToString() == "Admin")
                    { 
                        <li><a href="~/Home" style="color: #fff;">Home</a></li>
                        <li><a href="~/User" style="color: #fff;">User Management</a></li>
                        <li><a href="~/Report" style="color: #fff;">Report</a></li>
                        <li><a href="~/Incident" style="color: #fff;">Incident</a></li>
                    }
                    @if (Session["UserType"].ToString() == "Supervisor")
                    {
                        <li><a href="~/Home" style="color: #fff;">Home</a></li>
                        <li><a href="~/Report" style="color: #fff;">Report</a></li>
                    }
                    @if (Session["UserType"].ToString() == "Operator")
                    {
                        <li><a href="~/Home" style="color: #fff;">Home</a></li>
                    }
                     @if (Session["UserType"].ToString() == "Report")
                    {
                        <li><a href="~/Report" style="color: #fff;">Report</a></li>
                    }
                      @if (Session["UserType"].ToString() == "Recon")
                    {
                        <li><a href="~/Report" style="color: #fff;">Report</a></li>
                    }
                </ul>
            </div>
            <!--/.nav-collapse -->
        </div>
    </div>

    <div class="container-fluid">
        @*<div class="navbar-template text-center">*@
        <div class="navbar-template">
            @RenderBody()
        </div>
    </div>

 
</body>
</html>
