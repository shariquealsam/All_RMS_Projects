@* Upload File Design *@
<style>
    /*Tyre*/
.file-uploadTyre {display:block;text-align:center;font-family: Helvetica, Arial, sans-serif;font-size: 12px;}
.file-uploadTyre .file-selectTyre{display:block;border: 2px solid #dce4ec;color: #34495e;cursor:pointer;height:40px;line-height:40px;text-align:left;background:#FFFFFF;overflow:hidden;position:relative;}
.file-uploadTyre .file-selectTyre .file-selectTyre-button{background:#dce4ec;padding:0 10px;display:inline-block;height:40px;line-height:40px;}
.file-uploadTyre .file-selectTyre .file-selectTyre-name{line-height:40px;display:inline-block;padding:0 10px;}
.file-uploadTyre .file-selectTyre:hover{border-color:#34495e;transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-webkit-transition:all .2s ease-in-out;-o-transition:all .2s ease-in-out;}
.file-uploadTyre .file-selectTyre:hover .file-selectTyre-button{background:#34495e;color:#FFFFFF;transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-webkit-transition:all .2s ease-in-out;-o-transition:all .2s ease-in-out;}
.file-uploadTyre.active .file-selectTyre{border-color:#3fa46a;transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-webkit-transition:all .2s ease-in-out;-o-transition:all .2s ease-in-out;}
.file-uploadTyre.active .file-selectTyre .file-selectTyre-button{background:#3fa46a;color:#FFFFFF;transition:all .2s ease-in-out;-moz-transition:all .2s ease-in-out;-webkit-transition:all .2s ease-in-out;-o-transition:all .2s ease-in-out;}
.file-uploadTyre .file-selectTyre input[type=file]{z-index:100;cursor:pointer;position:absolute;height:100%;width:100%;top:0;left:0;opacity:0;filter:alpha(opacity=0);}
.file-uploadTyre .file-selectTyre.file-selectTyre-disabled{opacity:0.65;}
.file-uploadTyre .file-selectTyre.file-selectTyre-disabled:hover{cursor:default;display:block;border: 2px solid #dce4ec;color: #34495e;cursor:pointer;height:40px;line-height:40px;margin-top:5px;text-align:left;background:#FFFFFF;overflow:hidden;position:relative;}
