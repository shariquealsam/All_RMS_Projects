@* Search date between values of repair and maintaiance *@
<script>
    $(document).ready(function () {
        $("#SearchDetails").click(function () {
            $.ajax({
                url: '@Url.Action("SearchVehicle")',
                data: {
                    FromDate: $('#txtFromDate').val(),
                    ToDate: $('#txtToDate').val(),
                    RegionId: $('#cmbRegion').val(),
                    BranchId: $('#cmbBranch').val(),
                    VehicleNumber: $('#cmbVehicle').val(),
                },
                dataType: "json",
                type: "POST",
                traditional: true,
                success: function (data) {
                    $("#myDatatable").find("tr:not(:first)").remove();
                    var trHTML = '';

                    $.each(data, function (i, item) {

                        trHTML += '<tr><td style="display:none">' + data[i].RecId + '</td>' +
                                        '<td>' + data[i].VehicleNumber + '</td>' +
                                        '<td>' + data[i].ActualDateTime + '</td>' +
                                        '<td>' + data[i].Type + '</td>' +
                                        '<td>' + data[i].RegionName + '</td>' +
                                        '<td>' + data[i].BranchName + '</td>' +
                                        '<td>' + data[i].CreatedBy + '</td>' +
                                        '<td>' + data[i].TotalAmount + '</td>' +
                                        '<td> <button class="btn btn-primary" id="EditIdDetails"><span class="glyphicon glyphicon-edit"></span></button></td>' +
                                  '</tr>';
                    });
                    $('#myDatatable').append(trHTML);
                },
                error: function (response) {
                    alert(response.responseText);
                },
                failure: function (response) {
                    alert(response.responseText);
                }
            });
        });
    });
</script>

