@* Calculation Part *@
<script>
    $(document).ready(function () {
        $("#txtNoOfTyre,#txtTyreAmount,#txtNoOfBattery,#txtBattery").change(function () {
            var NoOfTyre = $('#txtNoOfTyre').val();
            var TyreAmount = $('#txtTyreAmount').val();
            var FinalAmount = (NoOfTyre * TyreAmount);
            $('#txtAmount').val(FinalAmount);

            var NoOfBattery = $('#txtNoOfBattery').val();
            var BatteryAmount = $('#txtBattery').val();
            var BatteryFinalAmount = (NoOfBattery * BatteryAmount);
            $('#txtBatteryAmount').val(BatteryFinalAmount);

        });
    });
</script>

