<script>
    function SetSessionValue(type)
    {
        //alert(type);
        //alert("d");
        var RecId = sessionStorage.getItem('RecId');
        $.ajax({
            url: '@Url.Action("SetSessionValue")',
            data: {
                RecId: RecId,
                Type: type,
                VehicleNo: $("#cmbVehicle").val(),
            },
            dataType: "json",
            type: "POST",
            traditional: true,
            success: function (data) {
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("ExportExcel", "Sales")', //call your controller and action
                        contentType: "application/pdf",
                        dataType: "json",
                    }).done(function (data) {
                        if (data.fileName != "") {
                            //use window.location.href for redirect to download action for download the file
                            window.location.href = "@Url.RouteUrl(new { Controller = "Sales", Action = "Download" })/?file=" + data.fileName;
                        }
                        else {
                            alert("No files found for the selected  segement!!");
                        }
                     });
                },
            error: function (response) {
                alert(response.responseText);
            },
            failure: function (response) {
                alert(response.responseText);
            }
        });

    }
</script>
