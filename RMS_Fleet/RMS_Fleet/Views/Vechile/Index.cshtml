<script>
      $(document).ready(function () {
          $('#cmbRegion').change(function () {
              $.ajax({
                  url: '@Url.Action("GetBranchdetails")',
                    data: {
                        RegionId: $('#cmbRegion').val(),
                    },
                    dataType: "json",
                    type: "POST",
                    traditional: true,
                    success: function (data) {
