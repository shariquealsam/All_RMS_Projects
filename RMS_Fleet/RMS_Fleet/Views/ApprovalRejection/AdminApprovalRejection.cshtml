                <div class="col-sm-2">
                    <label for="uRegion" class="control-label">Region</label>
                    <select class="form-control" id="cmbRegion" name="cmbRegion" required>
                        <option value = "0">--Select Region--</option>  
                         @foreach (var item1 in ViewBag.RegionDetails)
                         {
                            <option value="@item1.RegionId">@item1.RegionName</option>
                         }                 
                    </select> 
                      
                   
                    <script type="text/javascript">
                    $(document).ready(function () {

                        var strBranchId = '';

                        //Bank Dropdown Selectedchange event    
                        $('[id*=cmbRegion]').multiselect({
                            enableFiltering: true,
                            enableCaseInsensitiveFiltering: true,
                            includeSelectAllOption: false,
                            includeSelectAllDivider: false,
                            maxHeight: 260,
                            buttonWidth: '180px',
                            onChange: function () {
                                //alert($('#cmbRegion').val());
                                $.ajax({
                                    url: '@Url.Action("GetBranchdetails")',
                                    data: {
                                        RegionId: $('#cmbRegion').val(),
                                    },
                                    dataType: "json",
                                    type: "POST",
                                    traditional: true,
                                    success: function (data) {
                                        var mySelect = $('#cmbBranch');
                                        mySelect.empty();

                                        var mySelect1 = $('#cmbVehicle');
                                        mySelect1.empty();
                                        var selectOption = '<option value="0">Select Branch</option';
                                        $("#cmbBranch").append(selectOption);

                                        var selectOption = '<option value="">Select Vehicle</option';
                                        $("#cmbVehicle").append(selectOption);                                        
                                      
                                        
                                        $.each(data, function (i) {                                            
                                            var optionhtml = '';                                           
                                            optionhtml = '<option value="' + data[i].BranchId + '">' + data[i].BranchName + '</option>';
                                            $("#cmbBranch").append(optionhtml);

                                            strBranchId = data[0].BranchId;
                                         
                                           
                                            //selected
                                            $.ajax({
                                                url: '@Url.Action("GetVechileList")',
                                                data: {
                                                    BranchId: strBranchId,
                                                },

                                             
                                                dataType: "json",
                                                type: "POST",
                                                traditional: true,
                                                success: function (data) {
                                                    var mySelect = $('#cmbVehicle');
                                                    mySelect.empty();
                                                    var optionhtml = '';
                                                   
                                                    var selectOption = '<option value="">Select Vehicle</option';
                                                    $("#cmbVehicle").append(selectOption);                                                    
                                                    $.each(data, function (i) {
                                                        
                                                        optionhtml = '<option value="' + data[i].VehicleNumber + '">' + data[i].VehicleNumber + '</option>';
                                                        $('#cmbVehicle').append(optionhtml);
                                                    });
                                                    mySelect.multiselect('rebuild');
                                                    mySelect1.multiselect('rebuild');

                                                },
                                                error: function (response) {
                                                    alert(response.responseText);
                                                },
                                                failure: function (response) {
                                                    alert(response.responseText);
                                                }
                                            });
                                            //$('#cmbBranch').append(optionhtml);
                                        });
                                        mySelect.multiselect('rebuild');
                                        mySelect1.multiselect('rebuild');
                                    },
                                    error: function (response) {
                                        alert(response.responseText);
                                    },
                                    failure: function (response) {
                                        alert(response.responseText);
                                    }
                                });
                            },

                        });

                    });
                </script>      
                    
