@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Master.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Approval/Rejection</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.css">

    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.js"></script>

    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <style>
        [ng-cloak]{
            display:none !important;
        }
    </style>
</head>
<body>
    <div ng-cloak ng-app="myApp">
        <div class="container-fluid" ng-controller="ApprRejController">
            
            <h3 style="margin-top: -5px; font-weight: 600;"><i class="glyphicon glyphicon-ok"></i> Approval/Rejection 
            <span ng-show="loader" style="font-size:15px;color:firebrick;">Loading...<i class="fa fa-spinner fa-spin"></i></span></h3>
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="row">
                        <div class="col-sm-2">
                            <label for="uFromDate" class="control-label">From Date</label>
                            <input type="date" class="form-control" id="txtFromDate" name="txtFromDate" ng-model="fromdate" />
                        </div>
                        <div class="col-sm-2">
                            <label for="uToDate" class="control-label">To Date</label>
                            <input type="date" class="form-control" id="txtToDate" name="txtToDate" ng-model="todate" />
                        </div>
                        <div class="col-sm-2">
                            <label for="uRegion" class="control-label">Region</label>
                            <select class="form-control" id="cmbRegion" ng-model="regionId"
                                    ng-options="r.RegionId as r.RegionName for r in regions"
                                    ng-change="getBranchByRegion(regionId)">
                                <option value="">--Select Region--</option>
                            </select>
                        </div>
                        <div class="col-sm-2">
                            <label for="uBranch">Branch</label>
                            <select class="form-control" id="idBranch" name="cmbBranch" ng-model="branchId"
                                    ng-options="b.BranchId as b.BranchName for b in branches"
                                    ng-change="getVehicleByBranchId(branchId)">
                                <option value="">--Select Branch--</option>
                            </select>
                        </div>
                        <div class="col-sm-2">
                            <label for="uVehicleNumber">Vehicle Number</label>
                            <input type="text" list="autocompletelist" class="form-control" id="idVehicle" ng-model="VehicleNumber" placeholder="Vehicle Number" />
                            <datalist id="autocompletelist">
                                <option ng-repeat="vehicle in vehicles" value="{{vehicle.VehicleNumber}}"></option>
                            </datalist>
                        </div>
                        <div class="col-sm-2" style="margin-top:24px;">
                            <button class="btn btn-primary btn-block" id="SearchDetails" ng-click="getVehiclesByParam()">Search</button>
                        </div>
                    </div>
                    <hr />
                    <div class="row form-group">
                        <div class="col-md-12 ">
                            <ul class="nav nav-pills">
                                <li class="active bg-info" style="width:25%; text-align:center;" ng-click="getVehiclesByParam('Pending')"><a data-toggle="tab" href="#Pending">Pending From Admin- <span style="font-weight:bold;color:red;">({{lstVehiclePending.length}})</span></a></li>
                                <li class="bg-info" style="width:25%; text-align:center;" ng-show="userType=='SuperAdmin'" ng-click="getVehiclesByParam('PendingSA')"><a data-toggle="tab" href="#PendingSA">Pending For Approval- <span style="font-weight:bold;color:red;">({{lstVehicleRecommend.length}})</span></a></li>
                                <li class="bg-success" style="width:25%; text-align:center;" ng-show="userType=='Admin'" ng-click="getVehiclesByParam('Recommend')"><a data-toggle="pill" href="#Recommend">Recommend- <span style="font-weight:bold;color:red;">({{lstVehicleRecommend.length}})</span></a></li>
                                <li class="bg-success" style="width:25%; text-align:center;" ng-show="userType=='SuperAdmin' || userType=='User'" ng-click="getVehiclesByParam('Approved')"><a data-toggle="pill" href="#Approved">Approved- <span style="font-weight:bold;color:red;">({{lstVehicleApproved.length}})</span></a></li>
                                <li class="bg-danger" style="width:23%; text-align:center;" ng-show="userType=='Admin'" ng-click="getVehiclesByParam('Rejected')"><a data-toggle="pill" href="#Rejected">Rejected- <span style="font-weight:bold;color:red;">({{lstVehicleReject.length}})</span></a></li>
                                <li class="bg-danger" style="width:23%; text-align:center;" ng-show="userType=='SuperAdmin'" ng-click="getVehiclesByParam('RejectedSA')"><a data-toggle="pill" href="#RejectedSA">Rejected- <span style="font-weight:bold;color:red;">({{lstVehicleRejectSA.length}})</span></a></li>
                                <li class="bg-danger" style="width:23%; text-align:center;" ng-show="userType=='User'" ng-click="getVehiclesByParam('RejectedUser')"><a data-toggle="pill" href="#RejectedUser">Rejected- <span style="font-weight:bold;color:red;">({{lstVehicleRejectUser.length}})</span></a></li>
                            </ul>

                            <div class="tab-content">
                                <div id="Pending" class="tab-pane fade in active">
                                    <div style="height:500px;overflow-y:auto;">
                                        <table class="table table-bordered" style="margin-top:10px;">
                                            <thead class="bg-primary">
                                                <tr>
                                                    <th>Date</th>
                                                    <th>Vehicle Number</th>
                                                    <th>Type</th>
                                                    <th>Region</th>
                                                    <th>Branch</th>
                                                    <th>Total Amount</th>
                                                    <th>Created By</th>
                                                    <th ng-show="userType=='Admin'">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="v in lstVehiclePending">
                                                    <td>{{v.SalesDate}}</td>
                                                    <td>{{v.VehicleNumber}}</td>
                                                    <td>{{v.Type}}</td>
                                                    <td>{{v.RegionName}}</td>
                                                    <td>{{v.BranchName}}</td>
                                                    <td>{{v.TotalAmount}}</td>
                                                    <td>{{v.CreatedBy}}</td>
                                                    <td ng-show="userType=='Admin'">
                                                        <button type="button" class="btn btn-sm btn-info" data-target="#myModal" data-toggle="modal"
                                                                ng-click="getVehicleDetails(v)">
                                                            <i class="glyphicon glyphicon-edit"></i>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div id="PendingSA" class="tab-pane fade">
                                    <div class="row col-md-12">
                                        <button class="btn btn-success" type="button" ng-click="ApproveChecked()">Approve Checked</button>
                                    </div>
                                    <div class="row col-md-12">
                                        <div style="height:500px;overflow-y:auto;">
                                            <table class="table table-bordered" style="margin-top:10px;">
                                                <thead class="bg-primary">
                                                    <tr>
                                                        <th>
                                                            <input type="checkbox" ng-model="isChkAll" ng-change="SelectAll(isChkAll)" />
                                                        </th>
                                                        <th>Date</th>
                                                        <th>Vehicle Number</th>
                                                        <th>Type</th>
                                                        <th>Region</th>
                                                        <th>Branch</th>
                                                        <th>Total Amount</th>
                                                        <th>Created By</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="v in lstVehicleRecommend">
                                                        <td>
                                                            <input type="checkbox" ng-model="v.isCheck" />
                                                        </td>
                                                        <td>{{v.SalesDate}}</td>
                                                        <td>{{v.VehicleNumber}}</td>
                                                        <td>{{v.Type}}</td>
                                                        <td>{{v.RegionName}}</td>
                                                        <td>{{v.BranchName}}</td>
                                                        <td>{{v.TotalAmount}}</td>
                                                        <td>{{v.CreatedBy}}</td>
                                                        <td>
                                                            <button type="button" class="btn btn-sm btn-info" data-target="#myModalRecommendSA" data-toggle="modal"
                                                                    ng-click="getVehicleDetailsForSA(v)">
                                                                <i class="glyphicon glyphicon-edit"></i>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div id="Recommend" class="tab-pane fade">
                                    <div style="height:500px; overflow-y:auto;">
                                        <table class="table table-bordered" style="margin-top:10px;">
                                            <thead class="bg-primary">
                                                <tr>
                                                    <th>Date</th>
                                                    <th>Vehicle Number</th>
                                                    <th>Type</th>
                                                    <th>Region</th>
                                                    <th>Branch</th>
                                                    <th>Total Amount</th>
                                                    <th>Created By</th>
                                                    <!--<th>Action</th>-->
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="v in lstVehicleRecommend">
                                                    <td>{{v.SalesDate}}</td>
                                                    <td>{{v.VehicleNumber}}</td>
                                                    <td>{{v.Type}}</td>
                                                    <td>{{v.RegionName}}</td>
                                                    <td>{{v.BranchName}}</td>
                                                    <td>{{v.TotalAmount}}</td>
                                                    <td>{{v.CreatedBy}}</td>
                                                    <!--<td>
                            <button type="button" class="btn btn-sm btn-info" data-target="#myModal" data-toggle="modal"
                                    ng-click="getVehicleDetails(v)">
                                <i class="glyphicon glyphicon-edit"></i>
                            </button>
                        </td>-->
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div id="Approved" class="tab-pane fade">
                                    <div style="height:500px; overflow-y:auto;">
                                        <table class="table table-bordered" style="margin-top:10px;">
                                            <thead class="bg-primary">
                                                <tr>
                                                    <th>Date</th>
                                                    <th>Vehicle Number</th>
                                                    <th>Type</th>
                                                    <th>Region</th>
                                                    <th>Branch</th>
                                                    <th>Total Amount</th>
                                                    <th>Created By</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="v in lstVehicleApproved">
                                                    <td>{{v.SalesDate}}</td>
                                                    <td>{{v.VehicleNumber}}</td>
                                                    <td>{{v.Type}}</td>
                                                    <td>{{v.RegionName}}</td>
                                                    <td>{{v.BranchName}}</td>
                                                    <td>{{v.TotalAmount}}</td>
                                                    <td>{{v.CreatedBy}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div id="RejectedSA" class="tab-pane fade">
                                    <table class="table table-bordered" style="margin-top:10px;">
                                        <thead class="bg-primary">
                                            <tr>
                                                <th>Date</th>
                                                <th>Vehicle Number</th>
                                                <th>Type</th>
                                                <th>Region</th>
                                                <th>Branch</th>
                                                <th>Total Amount</th>
                                                <th>Created By</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="v in lstVehicleRejectSA">
                                                <td>{{v.SalesDate}}</td>
                                                <td>{{v.VehicleNumber}}</td>
                                                <td>{{v.Type}}</td>
                                                <td>{{v.RegionName}}</td>
                                                <td>{{v.BranchName}}</td>
                                                <td>{{v.TotalAmount}}</td>
                                                <td>{{v.CreatedBy}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div id="Rejected" class="tab-pane fade">
                                    <table class="table table-bordered" style="margin-top:10px;">
                                        <thead class="bg-primary">
                                            <tr>
                                                <th>Date</th>
                                                <th>Vehicle Number</th>
                                                <th>Type</th>
                                                <th>Region</th>
                                                <th>Branch</th>
                                                <th>Total Amount</th>
                                                <th>Created By</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="v in lstVehicleReject">
                                                <td>{{v.SalesDate}}</td>
                                                <td>{{v.VehicleNumber}}</td>
                                                <td>{{v.Type}}</td>
                                                <td>{{v.RegionName}}</td>
                                                <td>{{v.BranchName}}</td>
                                                <td>{{v.TotalAmount}}</td>
                                                <td>{{v.CreatedBy}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div id="RejectedUser" class="tab-pane fade">
                                    <table class="table table-bordered" style="margin-top:10px;">
                                        <thead class="bg-primary">
                                            <tr>
                                                <th>Date</th>
                                                <th>Vehicle Number</th>
                                                <th>Type</th>
                                                <th>Region</th>
                                                <th>Branch</th>
                                                <th>Total Amount</th>
                                                <th>Created By</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="v in lstVehicleRejectUser">
                                                <td>{{v.SalesDate}}</td>
                                                <td>{{v.VehicleNumber}}</td>
                                                <td>{{v.Type}}</td>
                                                <td>{{v.RegionName}}</td>
                                                <td>{{v.BranchName}}</td>
                                                <td>{{v.TotalAmount}}</td>
                                                <td>{{v.CreatedBy}}</td>
                                                <td>
                                                    <button type="button" class="btn btn-info" ng-click="ResendForApproval(v)">Re-Send</button>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal -->
            <div id="myModal" class="modal fade" role="dialog">
                <div class="modal-dialog" style="width:85%;">
                    <!-- Modal content-->
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                            <h4 class="modal-title">Vehicle Details</h4>
                        </div>
                        <div class="modal-body">
                            <!--Routine Service details-->
                            <div class="panel panel-default" ng-show="salesdet.RoutineAmount>0 && apprdet.RoutineStatus_Admin==''">
                                <div class="panel-heading" style="font-size: 15px; font-weight: 600; font-style: italic;">
                                    <span>Routine Service Details Below:</span>
                                    <span class="pull-right mr-5"><input type="checkbox" ng-model="apprdet.RoutineStatus_RH" />Select </span>
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-sm-2">
                                            <label for="uRoutineVendorName" class="control-label">Vendor Name</label>
                                            <input type="text" class="form-control" placeholder="Vendor Name" id="txtRoutineVendorName"
                                                   name="txtRoutineVendorName" ng-model="salesdet.RoutineVendorName" ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uDealerType" class="control-label">Dealer Type</label>
                                            <select class="form-control" id="cmbDealerType" name="cmbDealerType"
                                                    ng-disabled="true" ng-model="salesdet.RoutineDealerType">
                                                <option></option>
                                                <option>Authorized</option>
                                                <option>UnAuthorized</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutineInvoiceNo" class="control-label">Invoice No</label>
                                            <input type="text" class="form-control" placeholder="Invoice. No"
                                                   id="txtRoutineInvoiceNo" ng-model="salesdet.RoutineInvoiceNo"
                                                   ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutineInvoiceNo" class="control-label">Invoice Date</label>
                                            <input type="date" class="form-control" placeholder="Invoice Date" id="txtRoutineInvoiceDate"
                                                   ng-model="salesdet.RoutineInvoiceDate" ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutineKMReading" class="control-label">KM Reading</label>
                                            <input type="text" class="form-control" placeholder="KM Reading" id="txtRoutineKMReading"
                                                   ng-model="salesdet.RoutineKMReading" ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutinePartNumber" class="control-label">Part Number</label>
                                            <input type="text" class="form-control" placeholder="Part Number" id="txtRoutinePartNumber"
                                                   ng-model="salesdet.RoutinePartNumber" ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutineAmount" class="control-label">Amount Without GST</label>
                                            <input type="text" class="form-control" placeholder="Amount Without GST" id="txtRoutineAmount"
                                                   ng-model="salesdet.RoutineAmount" required="required"
                                                   ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutineGSTAmount" class="control-label">GST Amount</label>
                                            <input type="text" class="form-control" placeholder="GST Amount" id="txtRoutineGSTAmount"
                                                   ng-model="salesdet.RoutineGSTAmount" required="required"
                                                   ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutineTotalAmount" class="control-label">Total Amount</label>
                                            <input type="text" class="form-control" placeholder="Total Amount" id="txtRoutineTotalAmount"
                                                   ng-model="salesdet.RoutineTotalAmount" ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRoutineRemarks" class="control-label">Narration</label>
                                            <input type="text" class="form-control" placeholder="Remarks" id="txtRoutineRemarks"
                                                   ng-model="salesdet.RoutineRemarks" ng-disabled="true">
                                        </div>
                                        <div class="col-sm-2">
                                            <div class="col-sm-2">
                                                <label for="uRemarks" class="control-label" style="visibility: hidden;">NarrationNarration</label>
                                                <button class="btn fa fa-download" ng-click="SetSessionValue('Routine',apprdet)"></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!--Typre Service details-->
                            <div class="panel panel-default" ng-show="salesdet.TyreAmount>0 && apprdet.TyreStatus_Admin==''">
                                <div class="panel-heading" style="font-size: 15px; font-weight: 600; font-style: italic;">
                                    <span>Type Details Below:</span>
                                    <span class="pull-right mr-5"><input type="checkbox" ng-model="apprdet.TyreStatus_RH" />Select </span>
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-sm-2">
                                            <label for="uNoOfTyre" class="control-label">No Of Tyre</label>
                                            <input type="text" class="form-control" placeholder="No Of Tyre" id="txtNoOfTyre"
                                                   ng-model="salesdet.Tyre"
                                                   ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uTyre" class="control-label">Per Tyre Cost (RS.)</label>
                                            <input type="text" class="form-control" placeholder="Tyre  (RS.)" id="txtTyreAmount"
                                                   ng-model="salesdet.TyreAmount"
                                                   ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uCompanyName" class="control-label">Company Name</label>
                                            <input type="text" class="form-control" placeholder="Company Name" id="txtCompanyName"
                                                   ng-model="salesdet.TyreCompanyName"
                                                   ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uVendorName" class="control-label">Vendor Name</label>
                                            <input type="text" class="form-control" placeholder="Vendor Name" id="txtVendorName"
                                                   ng-model="salesdet.TyreVendorName"
                                                   ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uDealerType" class="control-label">Dealer Type</label>
                                            <select class="form-control" id="cmbTyerDealerType" ng-model="salesdet.TyerDealerTyp" ng-disabled="true">
                                                <option></option>
                                                <option>Authorized</option>
                                                <option>UnAuthorized</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uTyreSize" class="control-label">Tyre Size</label>
                                            <input type="text" class="form-control" placeholder="Tyre Size" id="txtTyreSize"
                                                   ng-model="salesdet.TyreSize"
                                                   ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uOriginalCost" class="control-label">Total Amount</label>
                                            <input type="text" class="form-control" placeholder="Total Amount" id="txtTyreTotalAmount"
                                                   ng-model="salesdet.TyreTotalAmount" ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRemarks" class="control-label">Narration</label>
                                            <input type="text" class="form-control" placeholder="Remarks" id="txtRemarks"
                                                   ng-model="salesdet.TyreRemarks" ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uRemarks" class="control-label" style="visibility:hidden;">NarrationNarration</label>
                                            <button class="btn fa fa-download" id="btnTyre" ng-click="SetSessionValue('Tyre',apprdet)" name="Tyre"></button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!--Battery Service details-->
                            <div class="panel panel-default" ng-show="salesdet.BatteryAmount>0 && apprdet.BatteryStatus_Admin==''">
                                <div class="panel-heading" style="font-size: 15px; font-weight: 600; font-style: italic;">
                                    <span> Battery Details Below:</span>
                                    <span class="pull-right mr-5"><input type="checkbox" ng-model="apprdet.BatteryStatus_RH" />Select </span>
                                </div>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-sm-2">
                                            <label for="uNoOfBattery" class="control-label">No. Of Battery</label>
                                            <input type="text" class="form-control" placeholder="No. Of Battery" id="txtNoOfBattery"
                                                   ng-model="salesdet.Battery" ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uBattery" class="control-label">Battery (RS.)</label>
                                            <input type="text" class="form-control" placeholder="Battery (RS.)" id="txtBattery"
                                                   ng-model="salesdet.BatteryAmount" ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uBatteryCompanyName" class="control-label">Company Name</label>
                                            <input type="text" class="form-control" placeholder="Company Name"
                                                   ng-model="salesdet.BatteryCompanyName" ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uBatteryVendorName" class="control-label">Vendor Name</label>
                                            <input type="text" class="form-control" placeholder="Vendor Name"
                                                   ng-model="salesdet.BatteryVendorName" ng-disabled="true" />
                                        </div>
                                        <div class="col-sm-2">
                                            <label for="uDealerType" class="control-label">Dealer Type</label>
                                            <select class="form-control" id="cmbBatteryDealerType" ng-model="salesdet.BatteryDealerType" ng-disabled="true">
                                                <option></option>
                                                <option>Authorized</option>
