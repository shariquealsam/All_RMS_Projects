                                    <label for="uReason" class="control-label">SuperAdmin Remarks</label>
                                    <input type="text" class="form-control" rows="2" placeholder="Reason" id="txtReason"
                                           ng-hide="userType !='SuperAdmin'" ng-model="apprdet.reason_sadmin" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <label for="extraEmail" class="control-label">Email</label>
                                    <input type="text" class="form-control" id="extraEmail" placeholder="Enter Email Id" ng-model="apprdet.optionalEmail" />
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12">
                                    <hr />
                                    <button class="btn btn-primary" type="button" ng-click="ApproveEntryById()">Submit <i ng-show="loader" class="fa fa-spinner fa-spin"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <script src="~/Angular/Angular.js"></script>
    <script src="~/Angular/myApp.js"></script>
    <script src="~/Angular/ApprovalRejectCtrl.js"></script>
    <script src="~/Js/table2excel.js"></script>
</body>
</html>
